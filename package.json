{
  "name": "cloudstash",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "email:dev": "email dev -d src/cf-worker/email/templates -p 3001",
    "dev:tunnel": "bun scripts/telegram-tunnel.ts",
    "dev:dashboard": "localflare attach --dev",
    "build": "vite build",
    "build:prod": "wrangler d1 migrations apply cloudstash --remote && vite build",
    "preview": "vite preview",
    "deploy": "wrangler d1 migrations apply cloudstash --remote && wrangler deploy",
    "deploy:staging": "wrangler d1 migrations apply cloudstash-staging --env staging --remote && wrangler deploy --env staging",
    "db:migrate:local": "wrangler d1 migrations apply cloudstash --local",
    "db:migrate:remote": "wrangler d1 migrations apply cloudstash --remote",
    "db:migrate:staging": "wrangler d1 migrations apply cloudstash-staging --env staging --remote",
    "db:generate": "drizzle-kit generate",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "typecheck": "tsgo --noEmit",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@base-ui/react": "1.1.0",
    "@fontsource-variable/jetbrains-mono": "5.2.8",
    "@livestore/adapter-cloudflare": "0.4.0-dev.22",
    "@livestore/adapter-web": "0.4.0-dev.22",
    "@livestore/livestore": "0.4.0-dev.22",
    "@livestore/peer-deps": "0.4.0-dev.22",
    "@livestore/react": "0.4.0-dev.22",
    "@livestore/sync-cf": "0.4.0-dev.22",
    "@livestore/wa-sqlite": "0.4.0-dev.22",
    "@react-email/body": "0.2.1",
    "@react-email/button": "0.2.1",
    "@react-email/container": "0.0.16",
    "@react-email/font": "0.0.10",
    "@react-email/head": "0.0.13",
    "@react-email/heading": "0.0.16",
    "@react-email/html": "0.0.12",
    "@react-email/preview": "0.0.14",
    "@react-email/render": "2.0.4",
    "@react-email/section": "0.0.17",
    "@react-email/tailwind": "2.0.3",
    "@react-email/text": "0.1.6",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "1.4.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "culori": "4.0.2",
    "domhandler": "5.0.3",
    "domutils": "3.2.2",
    "drizzle-orm": "0.45.1",
    "grammy": "1.39.3",
    "hono": "4.11.4",
    "htmlparser2": "10.0.0",
    "jose": "6.1.3",
    "lucide-react": "0.562.0",
    "motion": "12.27.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "5.2.3",
    "react-markdown": "10.1.0",
