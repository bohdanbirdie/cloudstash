{
  "name": "cloudstash",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "dev:tunnel": "bun scripts/telegram-tunnel.ts",
    "dev:dashboard": "localflare attach --dev",
    "build": "vite build",
    "build:prod": "wrangler d1 migrations apply cloudstash --env production --remote && vite build",
    "preview": "vite preview",
    "deploy": "wrangler d1 migrations apply cloudstash --env production --remote && wrangler deploy --env production",
    "deploy:staging": "wrangler d1 migrations apply cloudstash-staging --env staging --remote && wrangler deploy --env staging",
    "db:migrate:local": "wrangler d1 migrations apply cloudstash-local --local",
    "db:migrate:remote": "wrangler d1 migrations apply cloudstash --env production --remote",
    "db:migrate:staging": "wrangler d1 migrations apply cloudstash-staging --env staging --remote",
    "db:generate": "drizzle-kit generate",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --check .",
    "format:fix": "oxfmt --write .",
    "typecheck": "tsgo --noEmit"
  },
  "dependencies": {
    "@base-ui/react": "1.1.0",
    "@fontsource-variable/jetbrains-mono": "5.2.8",
    "@livestore/adapter-cloudflare": "0.4.0-dev.22",
    "@livestore/adapter-web": "0.4.0-dev.22",
    "@livestore/livestore": "0.4.0-dev.22",
    "@livestore/peer-deps": "0.4.0-dev.22",
    "@livestore/react": "0.4.0-dev.22",
    "@livestore/sync-cf": "0.4.0-dev.22",
    "@livestore/wa-sqlite": "0.4.0-dev.22",
    "@tailwindcss/vite": "^4.0.6",
    "@tanstack/react-router": "^1.132.0",
    "@tanstack/react-router-devtools": "^1.132.0",
    "@tanstack/router-plugin": "^1.132.0",
    "better-auth": "1.4.15",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cmdk": "1.1.1",
    "domhandler": "5.0.3",
    "domutils": "3.2.2",
    "drizzle-orm": "0.45.1",
    "grammy": "1.39.3",
    "hono": "4.11.4",
    "htmlparser2": "10.0.0",
    "jose": "6.1.3",
    "lucide-react": "0.562.0",
    "motion": "12.27.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hotkeys-hook": "5.2.3",
    "react-markdown": "10.1.0",
    "shadcn": "3.7.0",
    "swr": "2.3.8",
    "tailwind-merge": "3.4.0",
    "tailwindcss": "^4.0.6",
    "tw-animate-css": "1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zustand": "5.0.10"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.21.0",
    "@cloudflare/vitest-pool-workers": "0.12.5",
    "@cloudflare/workers-types": "4.20260118.0",
    "@livestore/adapter-node": "0.3.1",
    "@livestore/devtools-vite": "0.4.0-dev.22",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@typescript/native-preview": "^7.0.0-dev.20250518.1",
    "@vitejs/plugin-react": "^5.0.4",
    "drizzle-kit": "0.31.8",
    "jsdom": "^27.0.0",
    "localflare": "0.4.1",
    "oxfmt": "^0.25.0",
    "oxlint": "^0.16.6",
    "vite": "^7.1.7",
    "vitest": "^3.0.5",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.0.0"
  }
}
